pipeline {
    agent any
    environment {
        access = ''
    }
    stages {
        stage('first') {
            when {
                not {
                    allOf {
                        environment name: 'access', value: 'granted'
                        branch 'master'
                    }
                }
            }
            steps {
                echo "Just completed the first stage in ${env.BRANCH_NAME} ${BRANCH_NAME}"
            }
        }
    }
}
